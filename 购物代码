        for(var i=0; i<5; i++){
            if(min==5){
                min=0;
            }
            if(i==min){
                document.getElementById("img1").src=imgList[i];
                min++;
                if(i==0){
                    document.getElementById(li_imgList[i]).src="../素材/ok01.png";
