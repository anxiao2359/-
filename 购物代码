    //自动循环
    var int=self.setInterval("loop()",2000);
    function loop(){
        var str=document.getElementById("img2").src;
        for(var i=0; i<5; i++){
            if(min==5){
                min=0;
            }
            if(i==min){
                document.getElementById("img2").src=imgList[i];
                min++;
                if(i==0){
                    document.getElementById(li_imgList[i]).src="../素材/ok03.png";
                    document.getElementById(li_imgList[4]).src="../素材/ok04.png";
                }else{
                    document.getElementById(li_imgList[i]).src="../素材/ok03.png";
                    document.getElementById(li_imgList[i-1]).src="../素材/ok04.png";
                }
                break;
            }
        }
    }
    //鼠标触碰显示图片+暂停循环显示
    var a=3;
    function point(obj){
        clearInterval(int);
        //alert(obj.id);
        for(var i=0;i<5;i++){
            if(obj.id==liList[i]){
                document.getElementById("img2").src=imgList[i];
                min=i+1;
                document.getElementById(li_imgList[i]).src="../素材/ok03.png";
                for(var a=0;a<5;a++){
                    if(a!=i){
                        document.getElementById(li_imgList[a]).src="../素材/ok04.png";
                    }
                }
            }
        }
    }
    //鼠标离开继续循环
    function leave(){
        int = setInterval("loop()",2000);
    }
</script>
</html>

